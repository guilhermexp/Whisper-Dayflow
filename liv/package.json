{
  "name": "liv",
  "version": "0.1.7",
  "type": "module",
  "description": "AI powered dictation and journaling",
  "main": "./out/main/index.js",
  "author": "Liv",
  "homepage": "https://liv.app",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .cjs --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build && npm run build-rs",
    "postinstall": "node ./scripts/patch-entities.js && node ./scripts/patch-nan.js && electron-builder install-app-deps && node ./scripts/patch-panel-window.js",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --config electron-builder.config.cjs",
    "build:mac": "npm run build && electron-builder --mac --config electron-builder.config.cjs",
    "build:linux": "electron-vite build && electron-builder --linux --config electron-builder.config.cjs",
    "release": "node ./scripts/release.js",
    "fix-pnpm-windows": "node ./scripts/fix-pnpm-windows.js",
    "build-rs": "sh scripts/build-rs.sh"
  },
